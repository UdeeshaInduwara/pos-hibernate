<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="520.0" prefWidth="810.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.thogakadehibernate.controller.HomeController">
   <children>
      <VBox layoutX="20.0" layoutY="20.0" prefHeight="480.0" prefWidth="120.0" spacing="10.0" style="-fx-background-color: silver;">
         <children>
            <JFXButton fx:id="btnPlaceOrder" buttonType="RAISED" onAction="#placeOrder" prefHeight="48.0" prefWidth="120.0" text="Place Order" textFill="WHITE">
               <font>
                  <Font size="13.0" />
               </font></JFXButton>
            <JFXButton fx:id="btnCustomer" buttonType="RAISED" onAction="#customers" prefHeight="48.0" prefWidth="120.0" text="Customers" textFill="WHITE">
               <font>
                  <Font size="13.0" />
               </font></JFXButton>
            <JFXButton fx:id="btnItem" buttonType="RAISED" onAction="#items" prefHeight="48.0" prefWidth="120.0" text="Items" textFill="WHITE">
               <font>
                  <Font size="13.0" />
               </font></JFXButton>
         </children>
      </VBox>
      <AnchorPane fx:id="paneRoot" layoutX="150.0" prefHeight="520.0" prefWidth="660.0" />
   </children>
</AnchorPane>
